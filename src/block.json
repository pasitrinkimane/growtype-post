{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "growtype/post",
  "version": "0.1.0",
  "title": "Growtype Post",
  "category": "text",
  "icon": "flag",
  "description": "Use wp posts in advanced way.",
  "attributes": {
    "shortcode": {
      "type": "string",
      "source": "text",
      "selector": "div"
    },
    "content_source": {
      "type": "string",
      "default": "internal"
    },
    "content_url": {
      "type": "string",
      "default": ""
    },
    "content_url_cache": {
      "type": "boolean",
      "default": true
    },
    "ajax_load_content": {
      "type": "boolean",
      "default": false
    },
    "terms_navigation": {
      "type": "boolean",
      "default": false
    },
    "terms_navigation_taxonomy": {
      "type": "string",
      "default": "category"
    },
    "terms_navigation_style": {
      "type": "string",
      "default": "buttons"
    },
    "terms_navigation_style_mobile": {
      "type": "string",
      "default": "select"
    },
    "show_filters_visibility_trigger": {
      "type": "boolean",
      "default": false
    },
    "filters_visible_by_default": {
      "type": "boolean",
      "default": true
    },
    "terms_navigation_show_all_option_visible": {
      "type": "boolean",
      "default": false
    },
    "terms_navigation_selections_included_in_url": {
      "type": "boolean",
      "default": true
    },
    "terms_navigation_default_term_selected": {
      "type": "string",
      "default": ""
    },
    "terms_navigation_default_term_trigger_type": {
      "type": "string",
      "default": "click"
    },
    "terms_navigation_select_allow_multiple_options": {
      "type": "boolean",
      "default": false
    },
    "terms_navigation_select_allow_single_deselect": {
      "type": "boolean",
      "default": false
    },
    "post_type": {
      "type": "string",
      "default": "post"
    },
    "post__in": {
      "type": "string",
      "default": ""
    },
    "columns": {
      "type": "number",
      "default": 3
    },
    "load_all_posts": {
      "type": "boolean",
      "default": false
    },
    "show_load_more_posts_btn": {
      "type": "boolean",
      "default": false
    },
    "infinite_load_posts": {
      "type": "boolean",
      "default": false
    },
    "posts_per_page": {
      "type": "number",
      "default": 6
    },
    "posts_per_page_mobile": {
      "type": "number",
      "default": 6
    },
    "preview_style": {
      "type": "string",
      "default": "basic"
    },
    "preview_style_custom": {
      "type": "string",
      "default": ""
    },
    "parent_id": {
      "type": "string",
      "default": ""
    },
    "parent_class": {
      "type": "string",
      "default": ""
    },
    "post_is_a_link": {
      "type": "boolean",
      "default": true
    },
    "sticky_post": {
      "type": "string",
      "default": "none"
    },
    "post_in_modal": {
      "type": "boolean",
      "default": false
    },
    "order": {
      "type": "string",
      "default": "asc"
    },
    "orderby": {
      "type": "string",
      "default": "date"
    },
    "intro_content_length": {
      "type": "string",
      "default": "1000"
    },
    "show_if_no_posts": {
      "type": "boolean",
      "default": false
    },
    "show_custom_tax_posts": {
      "type": "boolean",
      "default": false
    },
    "custom_tax": {
      "type": "string",
      "default": ""
    },
    "custom_tax_level": {
      "type": "string",
      "default": "any"
    },
    "custom_tax_slug": {
      "type": "string",
      "default": ""
    },
    "pagination": {
      "type": "boolean",
      "default": false
    },
    "loading_type": {
      "type": "string",
      "default": "initial"
    },
    "meta_query": {
      "type": "string",
      "default": ""
    },
    "tax_query": {
      "type": "string",
      "default": ""
    },
    "custom_filters": {
      "type": "boolean",
      "default": false
    },
    "custom_filters_search_input_active": {
      "type": "boolean",
      "default": false
    },
    "custom_filters_orderby_select_active": {
      "type": "boolean",
      "default": false
    },
    "custom_filters_included": {
      "type": "string",
      "default": ""
    },
    "custom_filters_show_labels": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "html": false,
    "align": [
      "full"
    ]
  },
  "textdomain": "growtype-post",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
