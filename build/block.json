{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "growtype/post",
  "version": "0.1.0",
  "title": "Growtype Post",
  "category": "text",
  "icon": "flag",
  "description": "Use wp posts in advanced way.",
  "attributes": {
    "shortcode": {
      "type": "string",
      "source": "text",
      "selector": "div"
    },
    "content_source": {
      "type": "string",
      "default": "internal"
    },
    "content_url": {
      "type": "string",
      "default": ""
    },
    "content_url_cache": {
      "type": "boolean",
      "default": true
    },
    "terms_navigation": {
      "type": "boolean",
      "default": false
    },
    "terms_navigation_taxonomy": {
      "type": "string",
      "default": "category"
    },
    "post_type": {
      "type": "string",
      "default": "post"
    },
    "post__in": {
      "type": "string",
      "default": ""
    },
    "columns": {
      "type": "number",
      "default": 3
    },
    "load_all_posts": {
      "type": "boolean",
      "default": false
    },
    "posts_per_page": {
      "type": "number",
      "default": 6
    },
    "preview_style": {
      "type": "string",
      "default": "basic"
    },
    "preview_style_custom": {
      "type": "string",
      "default": ""
    },
    "parent_id": {
      "type": "string",
      "default": ""
    },
    "parent_class": {
      "type": "string",
      "default": ""
    },
    "post_is_a_link": {
      "type": "boolean",
      "default": true
    },
    "sticky_post": {
      "type": "string",
      "default": "none"
    },
    "post_in_modal": {
      "type": "boolean",
      "default": false
    },
    "order": {
      "type": "string",
      "default": "asc"
    },
    "orderby": {
      "type": "string",
      "default": "date"
    },
    "intro_content_length": {
      "type": "string",
      "default": "1000"
    },
    "pagination": {
      "type": "boolean",
      "default": false
    },
    "loading_type": {
      "type": "string",
      "default": "initial"
    },
    "meta_query": {
      "type": "string",
      "default": ""
    },
    "tax_query": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": false,
    "align": [
      "full"
    ]
  },
  "textdomain": "growtype-post",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}